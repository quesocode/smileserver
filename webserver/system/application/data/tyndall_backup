  `contact_city` varchar(50) default NULL,
  `contact_state` varchar(2) default NULL,
  `contact_zip` varchar(20) default NULL,
  `contact_office_hours` text,
  `lot_price_to_builder` double default NULL,
  `builder_take_down_date` datetime default NULL,
