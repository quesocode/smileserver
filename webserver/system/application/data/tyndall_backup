drop table if exists `Customers`;

CREATE TABLE `Customers` (
  `id` bigint(20) NOT NULL auto_increment,
  `username` varchar(50) default NULL,
  `password` varchar(255) default NULL,
  `first` varchar(50) default NULL,
  `last` varchar(50) default NULL,
  `email` varchar(255) default NULL,
  `address` varchar(50) default NULL,
  `address_line2` varchar(50) default NULL,
  `city` varchar(50) default NULL,
  `state` varchar(2) default NULL,
  `country` varchar(2) default NULL,
  `zip` varchar(10) default NULL,
  `phone` varchar(50) default NULL,
  `alt_phone` varchar(50) default NULL,
  `login_count` bigint(20) default '0',
  `title` varchar(10) default NULL,
  `login_attempts` smallint(6) default NULL,
  `last_login` datetime default NULL,
  `confirmation_code` varchar(255) default NULL,
  `created_at` datetime NOT NULL,
  `updated_at` datetime NOT NULL,
  `price_range_low` double default NULL,
  `price_range_high` double default NULL,
  `walkthroughs` bigint(20) default NULL,
  `timeframe` text,
  `amenity_location` varchar(10) default 'no',
  `amenity_schools` varchar(10) default 'no',
  `amenity_pool` varchar(10) default 'no',
  `amenity_park` varchar(10) default 'no',
  `amenity_recreation_center` varchar(10) default 'no',
  `amenity_community_center` varchar(10) default 'no',
  `amenity_trails` varchar(10) default 'no',
  `amenity_lakes` varchar(10) default 'no',
  `amenity_lake_lot` varchar(10) default 'no',
  `amenity_shopping` varchar(10) default 'no',
  `referred_by` text,
  `sales_person_id` bigint(20) default NULL,
  `referer_application` varchar(255) default NULL,
  `referer_url` varchar(255) default NULL,
  PRIMARY KEY  (`id`),
  KEY `sales_person_id_idx` (`sales_person_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `EmailTemplates`;

CREATE TABLE `EmailTemplates` (
  `id` bigint(20) NOT NULL auto_increment,
  `title` varchar(255) default NULL,
  `raw_html` text,
  `community_id` bigint(20) default NULL,
  `author_id` bigint(20) default NULL,
  `editor_id` bigint(20) default NULL,
  `date_created` datetime NOT NULL,
  `date_updated` datetime NOT NULL,
  `slug` varchar(255) default NULL,
  PRIMARY KEY  (`id`),
  UNIQUE KEY `sluggable_idx` (`slug`),
  KEY `author_id_idx` (`author_id`),
  KEY `editor_id_idx` (`editor_id`),
  KEY `community_id_idx` (`community_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `EmailVariables`;

