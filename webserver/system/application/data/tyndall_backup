SET NAMES latin1;
SET FOREIGN_KEY_CHECKS = 0;

drop table if exists `AccountUserRoles`;

CREATE TABLE `AccountUserRoles` (
  `id` bigint(20) NOT NULL auto_increment,
  `au_id` bigint(20) default NULL,
  `role_id` bigint(20) default NULL,
  PRIMARY KEY  (`id`),
  KEY `au_id_idx` (`au_id`),
  KEY `role_id_idx` (`role_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `AccountUsers`;

CREATE TABLE `AccountUsers` (
  `id` bigint(20) NOT NULL auto_increment,
  `account_id` bigint(20) default NULL,
  `user_id` bigint(20) default NULL,
  `date_created` datetime NOT NULL,
  `date_updated` datetime NOT NULL,
  PRIMARY KEY  (`id`),
  KEY `account_id_idx` (`account_id`),
  KEY `user_id_idx` (`user_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `Accounts`;

CREATE TABLE `Accounts` (
  `id` bigint(20) NOT NULL auto_increment,
  `name` varchar(50) default NULL,
  `description` text,
  `created_by_id` bigint(20) default NULL,
  `account_type` varchar(20) default NULL,
  `account_status` varchar(8) NOT NULL default 'active',
  `slug` varchar(255) default NULL,
  `date_created` datetime NOT NULL,
  `date_updated` datetime NOT NULL,
  PRIMARY KEY  (`id`),
  UNIQUE KEY `sluggable_idx` (`slug`)
) ENGINE=MyISAM AUTO_INCREMENT=5 DEFAULT CHARSET=latin1;

