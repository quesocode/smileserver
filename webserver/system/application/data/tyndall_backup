  `section` varchar(255) default NULL,
  `block` varchar(255) default NULL,
  `model_name` varchar(100) default NULL,
  `lot_number` varchar(100) default NULL,
  `lot_size` varchar(255) default NULL,
  `lot_type` varchar(255) default 'interior',
  `construction_startdate` datetime default NULL,
  `completion_date` datetime default NULL,
  `close_date` datetime default NULL,
  `address` varchar(100) default NULL,
  `address_line2` varchar(100) default NULL,
  `city` varchar(50) default NULL,
  `state` varchar(2) default NULL,
  `zip` varchar(20) default NULL,
  `price` double default NULL,
  `description` text,
  `square_footage` bigint(20) default NULL,
  `stories` int(11) default NULL,
  `bedrooms` int(11) default NULL,
  `bathrooms` int(11) default NULL,
  `garages` int(11) default NULL,
  `standard_features` text,
  `unique_features` text,
  `mls_number` varchar(255) default NULL,
  `lot_status` varchar(255) default 'for sale',
  `latitude` varchar(100) default NULL,
  `longitude` varchar(100) default NULL,
  `contact_company` varchar(100) default NULL,
  `contact_name` varchar(100) default NULL,
  `contact_phone` varchar(100) default NULL,
  `contact_email` varchar(100) default NULL,
  `contact_url` varchar(100) default NULL,
  `contact_company_url` varchar(100) default NULL,
