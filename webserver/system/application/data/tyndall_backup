drop table if exists `ApiVersionMethods`;

CREATE TABLE `ApiVersionMethods` (
  `id` bigint(20) NOT NULL auto_increment,
  `version_id` bigint(20) default NULL,
  `method_id` bigint(20) default NULL,
  PRIMARY KEY  (`id`),
  KEY `version_id_idx` (`version_id`),
  KEY `method_id_idx` (`method_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `ApiVersions`;

CREATE TABLE `ApiVersions` (
  `id` bigint(20) NOT NULL auto_increment,
  `display_name` text,
  `description` text,
  `slug` varchar(255) default NULL,
  `created_at` datetime NOT NULL,
  `updated_at` datetime NOT NULL,
  `version` bigint(20) default NULL,
  PRIMARY KEY  (`id`),
  UNIQUE KEY `sluggable_idx` (`slug`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `Communities`;

CREATE TABLE `Communities` (
  `id` bigint(20) NOT NULL auto_increment,
  `name` varchar(100) default NULL,
  `short_name` varchar(50) default NULL,
  `description` text,
  `account_id` bigint(20) default NULL,
  `slug` varchar(255) default NULL,
  `date_created` datetime NOT NULL,
  `date_updated` datetime NOT NULL,
  `map_tiles_folder` varchar(255) default NULL,
  `map_center_lat` varchar(50) default NULL,
  `map_center_lon` varchar(50) default NULL,
  `status` varchar(8) NOT NULL default 'active',
  `logo_id` bigint(20) default NULL,
  `thumb_id` bigint(20) default NULL,
  `icon_id` bigint(20) default NULL,
  `amenities` text,
  `schools` text,
  `hoa_info` text,
  `tax_rate` text,
  PRIMARY KEY  (`id`),
  UNIQUE KEY `sluggable_idx` (`slug`),
  KEY `account_id_idx` (`account_id`),
  KEY `logo_id_idx` (`logo_id`),
  KEY `thumb_id_idx` (`thumb_id`),
  KEY `icon_id_idx` (`icon_id`)
) ENGINE=MyISAM AUTO_INCREMENT=4 DEFAULT CHARSET=latin1;

