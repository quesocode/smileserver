CREATE TABLE `CommunityMedia` (
  `id` bigint(20) NOT NULL auto_increment,
  `community_id` bigint(20) default NULL,
  `media_id` bigint(20) default NULL,
  PRIMARY KEY  (`id`),
  KEY `community_id_idx` (`community_id`),
  KEY `media_id_idx` (`media_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `CommunityRoles`;

CREATE TABLE `CommunityRoles` (
  `id` bigint(20) NOT NULL auto_increment,
  `community_id` bigint(20) default NULL,
  `role_id` bigint(20) default NULL,
  PRIMARY KEY  (`id`),
  KEY `community_id_idx` (`community_id`),
  KEY `role_id_idx` (`role_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `CommunityUserRoles`;

CREATE TABLE `CommunityUserRoles` (
  `id` bigint(20) NOT NULL auto_increment,
  `cu_id` bigint(20) default NULL,
  `role_id` bigint(20) default NULL,
  PRIMARY KEY  (`id`),
  KEY `cu_id_idx` (`cu_id`),
  KEY `role_id_idx` (`role_id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


