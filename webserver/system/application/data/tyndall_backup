drop table if exists `account_index`;

CREATE TABLE `account_index` (
  `id` bigint(20) NOT NULL default '0',
  `keyword` varchar(200) NOT NULL default '',
  `field` varchar(50) NOT NULL default '',
  `position` bigint(20) NOT NULL default '0',
  PRIMARY KEY  (`id`,`keyword`,`field`,`position`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `api_method_index`;

CREATE TABLE `api_method_index` (
  `id` bigint(20) NOT NULL default '0',
  `keyword` varchar(200) NOT NULL default '',
  `field` varchar(50) NOT NULL default '',
  `position` bigint(20) NOT NULL default '0',
  PRIMARY KEY  (`id`,`keyword`,`field`,`position`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `api_method_version`;

CREATE TABLE `api_method_version` (
  `id` bigint(20) NOT NULL default '0',
  `name` text,
  `class_name` text,
  `description` text,
  `example` text,
  `function_type` varchar(255) default 'retrieve',
  `doc_type` varchar(255) default 'method',
  `created_by` bigint(20) NOT NULL,
  `updated_by` bigint(20) NOT NULL,
  `lft` int(11) default NULL,
  `rgt` int(11) default NULL,
  `level` smallint(6) default NULL,
  `slug` varchar(255) default NULL,
  `created_at` datetime NOT NULL,
  `updated_at` datetime NOT NULL,
  `version` bigint(20) NOT NULL default '0',
  PRIMARY KEY  (`id`,`version`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `api_version_version`;

CREATE TABLE `api_version_version` (
  `id` bigint(20) NOT NULL default '0',
  `display_name` text,
  `description` text,
  `slug` varchar(255) default NULL,
  `created_at` datetime NOT NULL,
  `updated_at` datetime NOT NULL,
  `version` bigint(20) NOT NULL default '0',
  PRIMARY KEY  (`id`,`version`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `community_index`;

CREATE TABLE `community_index` (
  `id` bigint(20) NOT NULL default '0',
  `keyword` varchar(200) NOT NULL default '',
  `field` varchar(50) NOT NULL default '',
  `position` bigint(20) NOT NULL default '0',
  PRIMARY KEY  (`id`,`keyword`,`field`,`position`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `lot_index`;

CREATE TABLE `lot_index` (
  `id` bigint(20) NOT NULL default '0',
  `keyword` varchar(200) NOT NULL default '',
  `field` varchar(50) NOT NULL default '',
  `position` bigint(20) NOT NULL default '0',
  PRIMARY KEY  (`id`,`keyword`,`field`,`position`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


drop table if exists `media_index`;

CREATE TABLE `media_index` (
  `id` bigint(20) NOT NULL default '0',
  `keyword` varchar(200) NOT NULL default '',
  `field` varchar(50) NOT NULL default '',
  `position` bigint(20) NOT NULL default '0',
  PRIMARY KEY  (`id`,`keyword`,`field`,`position`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


SET FOREIGN_KEY_CHECKS = 1;
